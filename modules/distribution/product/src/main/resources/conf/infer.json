{
  "deployment.server_role": {
    "gw": {
      "apim.traffic_manager.enable_publishing": true,
      "apim.traffic_manager.enable_deployer": false,
      "apim.traffic_manager.enable_blocking_conditions": true,
      "apim.traffic_manager.enable_decision_receiver": true,
      "apim.analytics.enable": true
    },
    "km": {
      "apim.traffic_manager.enable_publishing": false,
      "apim.traffic_manager.enable_deployer": false,
      "apim.traffic_manager.enable_blocking_conditions": false,
      "apim.traffic_manager.enable_decision_receiver": false,
      "apim.analytics.enable": false
    },
    "publisher": {
      "apim.traffic_manager.enable_publishing": true,
      "apim.traffic_manager.enable_deployer": true,
      "apim.traffic_manager.enable_blocking_conditions": false,
      "apim.traffic_manager.enable_decision_receiver": false,
      "apim.analytics.enable": true
    },
    "store": {
      "apim.traffic_manager.enable_publishing": false,
      "apim.traffic_manager.enable_deployer": false,
      "apim.traffic_manager.enable_blocking_conditions": false,
      "apim.traffic_manager.enable_decision_receiver": false,
      "apim.analytics.enable": true
    },
    "tm": {
      "apim.traffic_manager.enable_publishing": false,
      "apim.traffic_manager.enable_deployer": false,
      "apim.traffic_manager.enable_blocking_conditions": false,
      "apim.traffic_manager.enable_decision_receiver": false,
      "apim.analytics.enable": false
    }
  },
  "apim.jwt.generator_type": {
    "default": {
      "apim.jwt.generator_impl": "org.wso2.carbon.apimgt.keymgt.token.JWTGenerator",
      "apim.jwt.generator.enable": true
    },
    "url_safe": {
      "apim.jwt.generator_impl": "org.wso2.carbon.apimgt.keymgt.token.URLSafeJWTGenerator",
      "apim.jwt.generator.enable": true
    }
  },
  "apim.jwt.claims": {
    "default": {
      "apim.jwt.claims_extractor_impl": "org.wso2.carbon.apimgt.impl.token.DefaultClaimsRetriever"
    }
  },
  "apim.store.application_sharing_type": {
    "default": {
      "apim.store.application_sharing_impl": "org.wso2.carbon.apimgt.impl.DefaultGroupIDExtractorImpl"
    },
    "saml": {
      "apim.store.application_sharing_impl": "org.wso2.carbon.apimgt.impl.SAMLGroupIDExtractorImpl"
    }
  },
  "database.apim_db.type": {
    "mysql": {
      "database.apim_db.driver": "com.mysql.jdbc.Driver",
      "database.apim_db.url": "jdbc:mysql://$conf{database.shared_db.hostname}:$conf{database.shared_db.port}/$conf{database.shared_db.name}",
      "database.apim_db.validationQuery": "SELECT 1"
    },
    "h2": {
      "database.apim_db.driver": "org.h2.Driver",
      "database.apim_db.url": "jdbc:h2:./repository/database/WSO2CARBON_DB;DB_CLOSE_ON_EXIT\u003dFALSE;LOCK_TIMEOUT\u003d60000",
      "database.apim_db.validationQuery": "SELECT 1"
    }
  },
  "apim.oauth_config.encrypt_persisted_token": {
    "default": {
      "apim.oauth.encrypt_persisted_token": "true"
    }
  },
  "apim.oauth_config.token_hash_mode": {
    "default": {
      "apim.oauth.enable_token_hash_mode": "true"
    }
  },
  "apim.throttling.jms.factory.type": {
    "andes": {
      "apim.throttling.jms.java_naming_factory_initial": "org.wso2.andes.jndi.PropertiesFileInitialContextFactory"
    }
  },
  "apim.publisher.doc_visibility": {
    "enable": {
      "apim.publisher.enable_api_doc_visibility": true
    }
  }
}