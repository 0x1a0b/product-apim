<DataAgentsConfiguration>
    <Agent>
        <Name>{{databridge.agent.thrift.name}}</Name>
        <DataEndpointClass>{{databridge.agent.thrift.data_endpoint_class}}</DataEndpointClass>
        {% if databridge.agent.thrift.trust_store is defined %}
        <TrustSore>{{databridge.agent.thrift.trust_store}}</TrustSore>
        {% endif %}
        {% if databridge.agent.thrift.trust_store_password is defined %}
        <TrustSorePassword>{{databridge.agent.thrift.trust_store_password}}</TrustSorePassword>
        {% endif %}
        <QueueSize>{{databridge.agent.thrift.queue_size}}</QueueSize>
        <BatchSize>{{databridge.agent.thrift.batch_size}}</BatchSize>
        <CorePoolSize>{{databridge.agent.thrift.core_pool_size}}</CorePoolSize>
        <SocketTimeoutMS>{{databridge.agent.thrift.socket_timeout_ms}}</SocketTimeoutMS>
        <MaxPoolSize>{{databridge.agent.thrift.max_pool_size}}</MaxPoolSize>
        <KeepAliveTimeInPool>{{databridge.agent.thrift.keep_alive_time_in_pool}}</KeepAliveTimeInPool>
        <ReconnectionInterval>{{databridge.agent.thrift.reconnection_interval}}</ReconnectionInterval>
        <MaxTransportPoolSize>{{databridge.agent.thrift.max_transport_pool_size}}</MaxTransportPoolSize>
        <MaxIdleConnections>{{databridge.agent.thrift.max_idle_connections}}</MaxIdleConnections>
        <EvictionTimePeriod>{{databridge.agent.thrift.eviction_time_period}}</EvictionTimePeriod>
        <MinIdleTimeInPool>{{databridge.agent.thrift.min_idle_time_in_pool}}</MinIdleTimeInPool>
        <SecureMaxTransportPoolSize>{{databridge.agent.thrift.secure_max_transport_pool_size}}</SecureMaxTransportPoolSize>
        <SecureMaxIdleConnections>{{databridge.agent.thrift.secure_max_idle_connections}}</SecureMaxIdleConnections>
        <SecureEvictionTimePeriod>{{databridge.agent.thrift.secure_eviction_time_period}}</SecureEvictionTimePeriod>
        <SecureMinIdleTimeInPool>{{databridge.agent.thrift.secure_min_idle_time_in_pool}}</SecureMinIdleTimeInPool>
        {% if databridge.agent.thrift.ssl_enabled_protocols is defined %}
        <sslEnabledProtocols>{{databridge.agent.thrift.ssl_enabled_protocols}}</sslEnabledProtocols>
        {% endif %}
        {% if databridge.agent.thrift.ciphers is defined %}
        <ciphers>{{databridge.agent.thrift.ciphers}}</ciphers>
        {% endif %}
    </Agent>

    <Agent>
        <Name>{{databridge.agent.binary.name}}</Name>
        <DataEndpointClass>{{databridge.agent.binary.data_endpoint_class}}</DataEndpointClass>
        {% if databridge.agent.binary.trust_store is defined %}
        <TrustSore>{{databridge.agent.binary.trust_store}}</TrustSore>
        {% endif %}
        {% if databridge.agent.binary.trust_store_password is defined %}
        <TrustSorePassword>{{databridge.agent.binary.trust_store_password}}</TrustSorePassword>
        {% endif %}
        <QueueSize>{{databridge.agent.binary.queue_size}}</QueueSize>
        <BatchSize>{{databridge.agent.binary.batch_size}}</BatchSize>
        <CorePoolSize>{{databridge.agent.binary.core_pool_size}}</CorePoolSize>
        <SocketTimeoutMS>{{databridge.agent.binary.socket_timeout_ms}}</SocketTimeoutMS>
        <MaxPoolSize>{{databridge.agent.binary.max_pool_size}}</MaxPoolSize>
        <KeepAliveTimeInPool>{{databridge.agent.binary.keep_alive_time_in_pool}}</KeepAliveTimeInPool>
        <ReconnectionInterval>{{databridge.agent.binary.reconnection_interval}}</ReconnectionInterval>
        <MaxTransportPoolSize>{{databridge.agent.binary.max_transport_pool_size}}</MaxTransportPoolSize>
        <MaxIdleConnections>{{databridge.agent.binary.max_idle_connections}}</MaxIdleConnections>
        <EvictionTimePeriod>{{databridge.agent.binary.eviction_time_period}}</EvictionTimePeriod>
        <MinIdleTimeInPool>{{databridge.agent.binary.min_idle_time_in_pool}}</MinIdleTimeInPool>
        <SecureMaxTransportPoolSize>{{databridge.agent.binary.secure_max_transport_pool_size}}</SecureMaxTransportPoolSize>
        <SecureMaxIdleConnections>{{databridge.agent.binary.secure_max_idle_connections}}</SecureMaxIdleConnections>
        <SecureEvictionTimePeriod>{{databridge.agent.binary.secure_eviction_time_period}}</SecureEvictionTimePeriod>
        <SecureMinIdleTimeInPool>{{databridge.agent.binary.secure_min_idle_time_in_pool}}</SecureMinIdleTimeInPool>
        {% if databridge.agent.binary.ssl_enabled_protocols is defined %}
        <sslEnabledProtocols>{{databridge.agent.binary.ssl_enabled_protocols}}</sslEnabledProtocols>
        {% endif %}
        {% if databridge.agent.binary.ciphers is defined %}
        <ciphers>{{databridge.agent.binary.ciphers}}</ciphers>
        {% endif %}
    </Agent>
</DataAgentsConfiguration>

